{% extends 'admin.html.twig' %} {% block body %}
<div class="mt-5">
<!-- /. ROW  -->
        <div class="row">
            <div class="col-lg-12 ">
                <div class="mt-5 pt5">
                    {% for message in app.flashes('success') %}
                    <div class="alert alert-success">
                        {{ message }}
                    </div>
                    {% endfor %} {% for message in app.flashes('danger') %}
                    <div class="alert alert-danger">
                        {{ message }}
                    </div>
                    {% endfor %}
                </div>

            </div>
        </div>
        <!-- /. ROW  -->
    <div class="container mt-5">
        {{ form(form ) }}
    </div>
</div>
{% endblock %}